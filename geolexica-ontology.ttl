###  https://osgeo.geolexica.org/api/geolexica#abbreviation
:abbreviation rdf:type owl:DatatypeProperty ;
        rdfs:domain :ConceptInfo ;
        rdfs:range xsd:string ;
        dcterms:description "Abbreviation of concept."@en ;
        rdfs:label "has abbreviation"@en .

###  https://osgeo.geolexica.org/api/geolexica#classification
:classification rdf:type owl:DatatypeProperty ;
                rdfs:domain :ConceptInfo ;
                rdfs:range xsd:string ;
                dcterms:description "Classification of the term pertaining to concept INFO."@en ;
                rdfs:label "has INFO classification"@en ;
                skos:note "Need to enumerate classification values (i.e., \"preferred\", \"alternate\"...)."@en .

###  https://osgeo.geolexica.org/api/geolexica#clause
:clause rdf:type owl:DatatypeProperty ;
        rdfs:domain :ConceptInfo ;
        rdfs:range xsd:string ;
        dcterms:description "Classification of the term pertaining to concept INFO."@en ;
        rdfs:label "has SOURCE clause"@en .

###  https://osgeo.geolexica.org/api/geolexica#clause
:date-accepted rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:date ;
                     dcterms:description "Date the concept was accepted in concept REVIEW."@en ;
                     rdfs:label "date accepted"@en  .

###  https://osgeo.geolexica.org/api/geolexica#date-amended
:date-amended rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:date ;
        dcterms:description "Date the concept was amended in concept REVIEW."@en ;
        rdfs:label "has SOURCE clause"@en .

###  https://osgeo.geolexica.org/api/geolexica#decision
:decision rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          dcterms:description "Disposition of concept REVIEW."@en ;
          rdfs:label "has REVIEW decision"@en .

###  https://osgeo.geolexica.org/api/geolexica#decision-event
:decision-event rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                dcterms:description "Context statement of authority REVIEW."@en ;
                rdfs:label "has REVIEW decision event"@en .

###  https://osgeo.geolexica.org/api/geolexica#example
:example rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         dcterms:description "Example(s) to clarify concept definition."@en ;
         rdfs:label "has INFO example"@en .

###  https://osgeo.geolexica.org/api/geolexica#review-status
:info-status rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      dcterms:description "Status of a vocabulary term for Concept Info."@en ;
      rdfs:label "has info status"@en .

###  https://osgeo.geolexica.org/api/geolexica#jsonURI
:jsonURI rdf:type owl:DatatypeProperty ;
         rdfs:domain :JSON ;
         rdfs:range xsd:anyURI ;
         dcterms:description "JSON  property representation of resource."@en ;
         rdfs:label "has JSON API URI"@en ;
         skos:note "Property value is URI link to file."@en .

###  https://osgeo.geolexica.org/api/geolexica#jsonldURI
:jsonldURI rdf:type owl:DatatypeProperty ;
           rdfs:domain :JSON-LD ;
           rdfs:range xsd:anyURI ;
           dcterms:description "JSON-LD property representation of resource."@en ;
           rdfs:label "has JSON-LD API URI"@en ;
           skos:note "Property value is URI link to file."@en .

###  https://osgeo.geolexica.org/api/geolexica#last-review
:last-review rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:date ;
             dcterms:description "Date of the last REVIEW performed for the concept."@en ;
             rdfs:label "last review performed"@en .

###  https://osgeo.geolexica.org/api/geolexica#note
:note rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      dcterms:description "Notes pertaining to ConceptInfo or ConceptReview."@en ;
      rdfs:label "has note"@en .

###  https://osgeo.geolexica.org/api/geolexica#skosrdfURI
:skosrdfURI rdf:type owl:DatatypeProperty ;
            rdfs:domain :SKOS-RDF ;
            rdfs:range xsd:anyURI ;
            dcterms:description "SKOS-RDF property representation of a resource."@en ;
            rdfs:label "has SKOS-RDF API URI"@en ;
            skos:altLabel "has SKOS-IN-RDF API URI"@en ;
            skos:note "Property value is URI link to file."@en .

###  https://osgeo.geolexica.org/api/geolexica#review-status
:review-status rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string ;
      dcterms:description "Status of a vocabulary term for Concept Review."@en ;
      rdfs:label "has review status"@en .

###  https://osgeo.geolexica.org/api/geolexica#tbxisoxmlURI
:tbxisoxmlURI rdf:type owl:DatatypeProperty ;
              rdfs:domain :TBXISOXML ;
              rdfs:range xsd:anyURI ;
              dcterms:description "TBX-ISO-XML property representation of a resource."@en ;
              rdfs:label "has TBX-ISO-XML API URI"@en ;
              skos:note "Property value is URI link to file."@en .

###############################
#    Classes
###############################

###  http://purl.org/dc/terms/MediaType
dcterms:MediaType rdf:type owl:Class .

###  https://osgeo.geolexica.org/api/geolexica#ConceptInfo
:ConceptInfo rdf:type owl:Class ;
             dcterms:description "Instance data relative to INFO about selected concept."@en ;
             rdfs:comment "Attribute values for each concept in the glossary that indicates \"status\", \"classification\", and \"date-accepted\". New attributes may be added to future versions of the ontology."@en ;
             rdfs:label "Information"@en ;
             skos:prefLabel "Info"@en .

###  https://osgeo.geolexica.org/api/geolexica#ConceptReview
:ConceptReview rdf:type owl:Class ;
               dcterms:description "Instance data for official REVIEW of selected concept."@en ;
               rdfs:label "Review"@en ;
               rdfs:comment "Attribute values for each concept; i.e., \"last review performed\", \"status\", \"decision\", decision event\", and \"notes\". ."@en ;
               skos:prefLabel "Review"@en .

###  https://osgeo.geolexica.org/api/geolexica#JSON
:JSON rdf:type owl:Class ;
      rdfs:subClassOf :LinkedDataAPI ;
      dcterms:description "JSON file representation of concept."@en ;
      rdfs:label "JSON Linked Data API"@en ;
      skos:altLabel "JSON" .

###  https://osgeo.geolexica.org/api/geolexica#JSON-LD
:JSON-LD rdf:type owl:Class ;
         rdfs:subClassOf :LinkedDataAPI ;
         dcterms:description "JSON-LD file representation of concept"@en ;
         rdfs:label "JSON-LD Linked Data API"@en ;
         skos:altLabel "SKOS IN JSON-LD" .

###  https://osgeo.geolexica.org/api/geolexica#LangCode
:LangCode rdf:type owl:Class ;
          dcterms:description "ISO-3166 3-letter language code."@en ;
          rdfs:label "3-letter Language Code"@en ;
          rdfs:comment "Use for each language definition in the glossary" .

###  https://osgeo.geolexica.org/api/geolexica#LinkedDataAPI
:LinkedDataAPI rdf:type owl:Class ;
               rdfs:subClassOf dcterms:MediaType ;
               dcterms:description "Class for linked data APIs, distinct file formats for each concept."@en ;
               rdfs:label "Linked Data API"@en .

###  https://osgeo.geolexica.org/api/geolexica#Origin
:Origin rdf:type owl:Class ;
        dcterms:description "Source standards document for given language; may include location of chapter, paragraph, and section."@en ;
        rdfs:label "Origin"@en ;
        skos:note "Need to attach all Registers, all references as INDIVIDUALS."@en ,
                  "This should be a link to the Registers document."@en .

###  https://osgeo.geolexica.org/api/geolexica#SKOS-RDF
:SKOS-RDF rdf:type owl:Class ;
          rdfs:subClassOf :LinkedDataAPI ;
          dcterms:description "SKOS-RDF file representation of concept."@en ;
          rdfs:label "SKOS-RDF Linked Data API"@en ;
          skos:altLabel "SKOS IN RDF" .

###  https://osgeo.geolexica.org/api/geolexica#Source
:Source rdf:type owl:Class ;
        dcterms:description "Source standards document; may include location of chapter, paragraph, and section."@en ;
        rdfs:label "Source"@en ;
        skos:note "Need to attach all known Source documents as INDIVIDUALS."@en ,
                  "Should be a URL linking to standards document."@en .

###  https://osgeo.geolexica.org/api/geolexica#TBXISOXML
:TBXISOXML rdf:type owl:Class ;
           rdfs:subClassOf :LinkedDataAPI ;
           dcterms:description "TBX-ISO-XML file representation relative to selected concept."@en ;
           rdfs:label "TBX-ISO-XML Linked Data API"@en ;
           skos:altLabel "TBX-ISO-XML" ;
           skos:note "Is this supposed to be TBX-ISO-XML or -TML?"@en .

