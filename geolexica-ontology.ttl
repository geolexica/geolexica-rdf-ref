@base <https://osgeo.geolexica.org/api/geolexica> .

<https://osgeo.geolexica.org/api/geolexica> rdf:type owl:Ontology ;
         owl:versionIRI <https://osgeo.geolexica.org/api/geolexica/v1.0/> ;
         owl:imports <http://www.w3.org/2004/02/skos/core> ;
         dcterms:contributor <https://www.linkedin.com/in/rhtse/> ;
         dcterms:created "2022-01-12"^^xsd:date ;
         dcterms:creator <https://www.linkedin.com/in/lanemk> ;
         dcterms:description "This document presents the core ontology for OSGeo Geolexica, the online glossary for OSGeo."@en ;
         dcterms:issued " " ;
         dcterms:license "Copyright 2022 OSGeo" ;
         dcterms:modified "2022-01-25"^^xsd:date ;
         dcterms:publisher <https://www.osgeo.org/> ;
         dcterms:title "OSGeo Geolexica Ontology"@en ;
         vann:preferredNamespacePrefix "geolexica" ;
         vann:preferredNamespaceUri "https://osgeo.geolexica.org/api/geolexica#" ;
         owl:versionInfo "Version 1.0" ;
         skos:note "The Geolexica Ontology is currently under development as of 2022-19-01. Classes, properties, attributes, and axioms are subject to change."@en .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .

#################################################################
#    Object Properties
#################################################################

###  https://osgeo.geolexica.org/api/geolexica#hasInfo
:hasInfo rdf:type owl:ObjectProperty ;
         rdfs:domain skos:Concept ;
         rdfs:range :ConceptInfo ;
         dcterms:description "Object property for ConceptInfo values."@en ;
         rdfs:label "has info"@en .

###  https://osgeo.geolexica.org/api/geolexica#hasLangCode
:hasLangCode rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range :LangCode ;
             dcterms:description "Object property for LangCode values."@en ;
             rdfs:label "has language code"@en .

###  https://osgeo.geolexica.org/api/geolexica#hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
           rdfs:domain skos:Concept ;
           rdfs:range :Origin ;
           dcterms:description "Origin of defintion in ISO-TC 211 Registers"@en ;
           rdfs:label "has origin"@en .

###  https://osgeo.geolexica.org/api/geolexica#hasReview
:hasReview rdf:type owl:ObjectProperty ;
           rdfs:domain skos:Concept ;
           rdfs:range :ConceptReview ;
           dcterms:description "Object property for ConceptReview values."@en ;
           rdfs:label "has review"@en .

###  https://osgeo.geolexica.org/api/geolexica#hasSource
:hasSource rdf:type owl:ObjectProperty ;
           rdfs:domain skos:Concept ;
           rdfs:range :Source ;
           dcterms:description "Authority source for concept defintion."@en ;
           rdfs:label "has source"@en .

###  https://osgeo.geolexica.org/api/geolexica#hasLDAPI
:hasLDAPI rdf:type owl:ObjectProperty ;
           rdfs:domain skos:Concept ;
           rdfs:range :LinkedDataAPI ;
           dcterms:description "Object property for Linked Data APIs."@en ;
           rdfs:label "has Linked Data API"@en .

#############################
#    Data properties
#############################

###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:DatatypeProperty ;
                   rdfs:domain skos:Concept ;
                   rdfs:range xsd:integer ;
                   dcterms:description "Encodes the term ID for each concept."@en ;
                   rdfs:comment "Entry must be an integer."@en ;
                   rdfs:label "identifier"@en ;
                   skos:note "From the DCMI Metadata Terms vocabulary."@en .

###  http://www.linkedmodel.org/schema/vaem#abbreviation
vaem:abbreviation rdf:type owl:DatatypeProperty ;
                  rdfs:domain skos:Concept ;
                  rdfs:range xsd:string ;
                  dcterms:description "ISO 3166 three-letter language codes assigned to each translation."@en ;
                  rdfs:label "Abbreviation"@en ;
                  skos:note "From Vocabulary for Essential Metadata (VAEM)."@en .

