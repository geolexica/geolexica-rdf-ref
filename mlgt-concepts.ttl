skos:notation 1463 ;

  skos:definition "measured quantity value minus a reference quantity value"@en ;
  skos:definition "측정량값에서 참조량값을 제한 것"@ko ;
  skos:definition "nilai kuantiti diukur dibandingkan dengan nilai kuantiti rujukan"@ms ;
  skos:definition "измеренное значение величины минус значение контрольной величины"@ru ;
