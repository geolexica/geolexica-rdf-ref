:data_quality_result rdf:type owl:NamedIndividual ,
                              skos:Concept ;
                     skos:inScheme geolexica:GeolexicaConceptScheme ;
                     geolexica:hasInfo info:Info117 ;
                     geolexica:hasLDAPI ldapi:ldapi117-json ,
                                        ldapi:ldapi117-jsonld ,
                                        ldapi:ldapi117-rdfxml ,
