<rdf:Description rdf:about="https://isotc211.geolexica.org/concepts/2251/spatial_dimension_adj">
	<identifier xmlns="http://purl.org/dc/terms/" rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2251</identifier>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<label xmlns="http://www.w3.org/2000/01/rdf-schema#">spatial dimension, adj (topology, geometry)</label>
	<definition xmlns="http://www.w3.org/2004/02/skos/core#" xml:lang="en">number of independent decisions in a coordinate system required to locate a position</definition>
	<inScheme xmlns="http://www.w3.org/2004/02/skos/core#" rdf:resource="https://isotc211.geolexica.org/geolexica#GeolexicaConceptScheme"/>
