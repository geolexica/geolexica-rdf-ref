:quality_schema  a             skos:Concept , owl:NamedIndividual ;
        rdfs:label             "quality schema" ;
        dcterms:identifier     360 ;
        skos:definition        "концептуальная схема, определяющая аспекты, связанные с качеством пространственных    данных"@ru , "schéma conceptuel définissant les aspects qualité des données géographiques"@fr , "esquema conceptual que define aspectos de calidad de datos geográficos"@es , "مخطط تصويري يحدد جوانب الجودة للبيانات الجغرافية"@ar , "begreppsschema som definierar kvalitetsaspekter hos geodata"@sv , "paikkatiedon laatunäkökohtia kuvaava käsiteskeema"@fi , "定义地理数据质量内容的概念模式。"@zh , "conceptual schema defining aspects of quality for geographic data"@en , "지리 데이터의 품질 측면을 정의하는 개념적 스키마"@ko ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          360 ;
        skos:prefLabel         "مخطط الجودة"@ar , "质量模式"@zh , "quality schema"@en , "laatuskeema"@fi , "geodatakvalitetsschema"@sv , "Qualitätsschema"@de , "схема качества"@ru , "schéma qualité"@fr , "품질 스키마"@ko , "esquema de calidad"@es ;
        lang:deu-note          notes:DefnTransUnavail ;
        geolexica:hasInfo      info:Info360 ;
        geolexica:hasLDAPI     ldapi:ldapi360-json , ldapi:ldapi360-skosrdf , ldapi:ldapi360-rdfxml , ldapi:ldapi360-jsonld ;
        geolexica:hasLangCode  lang:kor , lang:eng , lang:jpn , lang:rus , lang:pol , lang:spa , lang:msa , lang:deu , lang:swe , lang:zho , lang:fra , lang:ara , lang:fin , lang:nld ;
        geolexica:hasOrigin    lang:deuRegistry , lang:araRegistry , lang:korRegistry , lang:engRegistry , lang:fraRegistry , lang:polRegistry , lang:msaRegistry , lang:sweRegistry , lang:zhoRegistry , lang:nldRegistry , lang:finRegistry , lang:spaRegistry , lang:jpnRegistry , lang:rusRegistry ;
        geolexica:hasReview    reviews:Review360 .

