:direct_geodetic_problem
        a                      skos:Concept , owl:NamedIndividual ;
        rdfs:label             "direct geodetic problem (differential geometry, geodesy)" ;
        dcterms:identifier     2160 ;
        skos:altLabel          "first geodetic problem (differential geometry, geodesy)"@en ;
        skos:definition        "problem that given a point on a surface and the direction and distance from that point to a second point along a geodesic, determines that second point"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2160 ;
        skos:prefLabel         "direct geodetic problem (differential geometry, geodesy)"@en ;
        lang:eng-note          "Note to entry: This \"problem\" defines a mapping from the vector space at a point (each vector given by a direction and a length) to points of the Figure of Earth that satisfy the problem for that direction and distance. For example, if we fix the distance \"r\" and take all directions, the resultant geometry is the circle centred at the original point of radius \"r\". This document will make heavy use of this mapping; see exponential map and the second geodetic problem. "@en ;
        geolexica:hasInfo      info:Info2160 ;
        geolexica:hasLDAPI     ldapi:ldapi2160-json , ldapi:ldapi2160-skosrdf , ldapi:ldapi2160-rdfxml , ldapi:ldapi2160-jsonld ;
        geolexica:hasLangCode  lang:ara , lang:jpn , lang:msa , lang:nld , lang:kor , lang:fra , lang:zho , lang:eng , lang:fin , lang:swe , lang:deu , lang:rus , lang:pol , lang:spa ;
        geolexica:hasOrigin    lang:korRegistry , lang:jpnRegistry , lang:engRegistry , lang:araRegistry , lang:deuRegistry , lang:msaRegistry , lang:nldRegistry , lang:sweRegistry , lang:fraRegistry , lang:spaRegistry , lang:finRegistry , lang:rusRegistry , lang:zhoRegistry , lang:polRegistry ;
        geolexica:hasReview    reviews:Review2160 .

