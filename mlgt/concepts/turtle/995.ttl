        geolexica:hasLDAPI     ldapi:ldapi995-jsonld , ldapi:ldapi995-rdfxml , ldapi:ldapi995-skosrdf , ldapi:ldapi995-json ;
        geolexica:hasLangCode  lang:zho , lang:swe , lang:kor , lang:msa , lang:deu , lang:pol , lang:fin , lang:jpn , lang:nld , lang:fra , lang:spa , lang:rus , lang:eng , lang:ara ;
