:topol_bound_geom_topol
        a                      skos:Concept , owl:NamedIndividual ;
        rdfs:label             "topological boundary (geometry, topology)" ;
        dcterms:identifier     2096 ;
        skos:definition        "boundary represented by a set of oriented topological primitives of smaller topological dimension that limits the extent of a topological object, or by the geometric realization of such a set"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2096 ;
        skos:prefLabel         "topological boundary (geometry, topology)"@en ;
        lang:eng-note          "Note to entry: The boundary of a topological complex is the boundary of the geometric realization of the topological complex."@en ;
        geolexica:hasInfo      info:Info2096 ;
        geolexica:hasLDAPI     ldapi:ldapi2096-skosrdf , ldapi:ldapi2096-rdfxml , ldapi:ldapi2096-json , ldapi:ldapi2096-jsonld ;
        geolexica:hasLangCode  lang:deu , lang:fra , lang:jpn , lang:ara , lang:zho , lang:spa , lang:rus , lang:eng , lang:nld , lang:kor , lang:msa , lang:fin , lang:swe , lang:pol ;
