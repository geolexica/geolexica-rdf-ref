:remote_sensing  a             owl:NamedIndividual , skos:Concept ;
        rdfs:label             "remote sensing" ;
        dcterms:identifier     379 ;
        skos:definition        "Pengumpulan dan intepretasi maklumat tentang sesuatu objek dengan menggunakan peranti perakaman yang tidak berhubungan secara fizikal dengan objek tersebut."@ms , "berührungslose Erfassung und Interpretation von Informationen über Objekte"@de , "개체와 물리적 접촉 없이 개체에 관해 정보를 수집하고 해석하는 행위"@ko , "collection and interpretation of information about an object without being in physical contact with the object"@en , "сбор и интерпретация информации об объекте без физического контакта с объектом"@ru , "adquisición e interpretación de información de un objeto sin que exista contacto físico con él"@es , "جمع وتفسير المعلومات عن كائن دون ان يكون هناك اتصال محسوس مع الكائن"@ar , "insamling och tolkning av information om en företeelse utan att vara i fysisk kontakt med densamma"@sv ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          379 ;
        skos:prefLabel         "дистанционное зондирование"@ru , "fjärranalys"@sv , "teledetección"@es , "원격 탐사"@ko , "Fernerkundung"@de , "penderiaan jauh"@ms , "استشعار عن بعد"@ar , "remote sensing"@en ;
        geolexica:hasInfo      info:Info379 ;
        geolexica:hasLDAPI     ldapi:ldapi379-json , ldapi:ldapi379-rdfxml , ldapi:ldapi379-jsonld , ldapi:ldapi379-skosrdf ;
        geolexica:hasLangCode  lang:nld , lang:spa , lang:eng , lang:kor , lang:swe , lang:zho , lang:fin , lang:jpn , lang:pol , lang:ara , lang:deu , lang:msa , lang:fra , lang:rus ;
        geolexica:hasOrigin    lang:araRegistry , lang:zhoRegistry , lang:spaRegistry , lang:nldRegistry , lang:deuRegistry , lang:fraRegistry , lang:msaRegistry , lang:engRegistry , lang:jpnRegistry , lang:polRegistry , lang:finRegistry , lang:korRegistry , lang:sweRegistry , lang:rusRegistry ;
        geolexica:hasReview    reviews:Review379 .

