:ordinal_temp_ref_system
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "ordinal temporal reference system" ;
        dcterms:identifier     323 ;
        skos:definition        "temporal reference system composed of ordinal eras"@en , "czasowy system odniesienia złożony z er porządkowych"@pl , "نظام مرجعي زمني مؤلف من حقب ترتيبية"@ar , "순서 연대의 계층 구조로 구성된 시간 참조 체계"@ko , "sistema de referencia temporal que consta de eras ordinales"@es , "ajan vertausjärjestelmä, joka koostuu ordinaaliajanjaksoista"@fi , "由顺序时代构成的时间参照系。"@zh , "temporala referenssystem bestående av ordinaleror"@sv , "système de référence temporel composé d&#39;ères ordinales"@fr , "система регистрации времени, составленная из порядковых эр"@ru , "順序年代の集合からなる時間参照系の一つ。順序尺度を基礎とする。"@ja ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          323 ;
        skos:prefLabel         "ordinalt tidsreferenssystem"@sv , "порядковая система отсчета времени"@ru , "porządkowy czasowy system odniesienia"@pl , "ordinaalinen ajan vertausjärjestelmä"@fi , "顺序时间参照系"@zh , "système de référence temporel ordinal"@fr , "zeitbezogenes Bezugssystem"@de , "順序時間参照系"@ja , "نظام مرجعي ترتيبي زمني"@ar , "서수 시간 참조 체계"@ko , "sistema de referencia temporal ordinal"@es , "ordinal temporal reference system"@en ;
        lang:lang-deu          notes:DefnTransUnavail ;
        lang:lang-fra          notes:NoteTransDiff ;
        geolexica:hasInfo      info:Info323 ;
        geolexica:hasLDAPI     ldapi:ldapi323-rdfxml , ldapi:ldapi323-jsonld , ldapi:ldapi323-json , ldapi:ldapi323-skosrdf ;
        geolexica:hasLangCode  lang:swe , lang:fra , lang:rus , lang:jpn , lang:eng , lang:spa , lang:zho , lang:kor , lang:deu , lang:nld , lang:fin , lang:msa , lang:pol , lang:ara ;
