:ordinal_era  a                skos:Concept , owl:NamedIndividual ;
        rdfs:label             "ordinal era" ;
        dcterms:identifier     321 ;
        skos:altLabel          "järjestysajanjakso"@fi ;
        skos:definition        "時間的に順序付けられた，名前をもつ期間。"@ja , "one of a set of named periods ordered in time"@en , "시대순으로 명명된 기간들 중 하나"@ko , "按时间排序命名的时段集合中的一个时段。"@zh , "واحدة من مجموعة حقب ذات أسماء مرتبه حسب الوقت"@ar , "un des ensembles de périodes nommées ordonnées dans le temps"@fr , "один из элементов набора именованных периодов, упорядоченных во времени"@ru , "uno de una serie de períodos ordenados en el tiempo"@es , "jeden ze zbiorów zlożonych z nazwanych okresów uporządkowanych w czasie"@pl , "nimettyjen, aikaan järjestettyjen ajanjaksojen joukon jakso"@fi , "en av flera tidsperioder i en sekvens"@sv ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          321 ;
        skos:prefLabel         "era porządkowa"@pl , "Ordinalepoche"@de , "ordinal era"@en , "порядковая эра"@ru , "ordinalera"@sv , "順序年代"@ja , "순서 연대"@ko , "ordinaaliajanjakso"@fi , "حقبة ترتيبية"@ar , "era ordinal"@es , "顺序时代"@zh , "ère ordinale"@fr ;
        lang:lang-deu          notes:DefnTransUnavail ;
        lang:lang-fra          notes:NoteTransDiff ;
