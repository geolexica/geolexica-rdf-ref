:object_point  a               owl:NamedIndividual , skos:Concept ;
        rdfs:label             "object point" ;
        dcterms:identifier     518 ;
        skos:definition        "Punkt im Objektraum, der von einem Sensor abgebildet wird"@de , "센서에 의해 영상화된 개체 공간의 점"@ko , "نقطة في حيز الكائن الذي تم تصويره بجهاز استشعار"@ar , "point in the object space that is imaged by a sensor"@en , "точка в пространстве объекта, отображаемая датчиком"@ru , "punto en el espacio del objeto que es capturado por un sensor"@es , "lägesbestämd punkt som avbildas av en sensor"@sv ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          518 ;
        skos:prefLabel         "punto objeto"@es , "물체점"@ko , "object point"@en , "avbildad punkt"@sv , "Objektpunkt"@de , "نقطة كائن"@ar , "точка объекта"@ru ;
        lang:ara-note          "ملاحظة: في الاستشعار عن بعد أو المسح الجوي ، نقطة الكيان أو الشيء هي نقطة محددة في نظام إحداثيات أرضي."@ar ;
        lang:deu-note          notes:NoteTransUnavail ;
        lang:eng-note          "Note to entry: In remote sensing and aerial photogrammetry an object point is a point defined in an Earth-fixed coordinate reference system."@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:rus-note          "Примечание: в дистанционном зондировании и аэрофотограмметрии точкой объекта является точка, определенная в системе координат, фиксированной на Земле."@ru ;
        lang:spa-note          "Nota: En teledetección y en fotogrametría aérea un punto objeto es un punto definido en un sistema de referencia de coordenadas fijo en la Tierra."@es ;
        lang:swe-note          notes:NoteTransUnavail ;
        geolexica:hasInfo      info:Info518 ;
        geolexica:hasLDAPI     ldapi:ldapi518-jsonld , ldapi:ldapi518-skosrdf , ldapi:ldapi518-json , ldapi:ldapi518-rdfxml ;
        geolexica:hasLangCode  lang:fin , lang:swe , lang:spa , lang:kor , lang:nld , lang:zho , lang:pol , lang:msa , lang:ara , lang:eng , lang:fra , lang:jpn , lang:rus , lang:deu ;
