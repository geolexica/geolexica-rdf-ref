:image_coordinates  a          owl:NamedIndividual , skos:Concept ;
        rdfs:label             "image coordinates" ;
        dcterms:identifier     1395 ;
        skos:definition        "coordenadas con respecto al sistema de coordenadas cartesiano de una imagen"@es , "coordinates with respect to a Cartesian coordinate system of an image"@en , "koordinater enligt en bilds kartesiska koordinatsystem"@sv , "координаты относительно декартовой системы координат изображения"@ru , "영상의 데카르트 좌표계에 대한 좌표"@ko ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1395 ;
        skos:prefLabel         "image coordinates"@en , "Координаты изображения"@ru , "coordenadas de imagen"@es , "영상 좌표"@ko , "bildkoordinater"@sv ;
        lang:eng-note          "Note to entry: The image coordinates can be in pixels or in a measure of length (linear measure)."@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:rus-note          "Примечание: координаты изображения могут быть в пикселях или в мере длины (линейная мера)."@ru ;
