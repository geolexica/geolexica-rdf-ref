:incident_angle  a             skos:Concept , owl:NamedIndividual ;
        rdfs:label             "incident angle" ;
        dcterms:identifier     954 ;
        skos:definition        "탐지된 요소가 센서와 지역 표면 법선 사이에 맞닿는 수선의 맞꼭지각(접선면 법선)"@ko , "Winkel zwischen dem Lot und der Messrichtung des Radarstrahls"@de , "вертикальный угол между линией от обнаруженного элемента до датчика и нормалью локальной поверхности (нормалью касательной плоскости)"@ru , "vertical angle between the line from the detected element to the sensor and the local surface normal (tangent plane normal)"@en , "زاوية عمودية بين الخط الممتد من العنصر المكتشف إلى الحساس والسطح المحلي المستوي (السطح المستَوِي المماس)."@ar , "vinkeln mellan en stråle som faller på en yta och normalen till ytan vid infallspunkten."@sv , "ángulo vertical entre la línea procedente del elemento detectado hasta el sensor y la superficie local normal (plano tangente normal)"@es ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          954 ;
        skos:prefLabel         "Угол падения"@ru , "incident angle"@en , "Einfallswinkel"@de , "ángulo de incidencia"@es , "زاوية السقوط"@ar , "입사각"@ko , "infallsvinkel"@sv ;
        geolexica:hasInfo      info:Info954 ;
        geolexica:hasLDAPI     ldapi:ldapi954-rdfxml , ldapi:ldapi954-skosrdf , ldapi:ldapi954-jsonld , ldapi:ldapi954-json ;
        geolexica:hasLangCode  lang:swe , lang:msa , lang:zho , lang:spa , lang:nld , lang:rus , lang:fin , lang:kor , lang:ara , lang:jpn , lang:deu , lang:fra , lang:eng , lang:pol ;
        geolexica:hasOrigin    lang:araRegistry , lang:nldRegistry , lang:spaRegistry , lang:finRegistry , lang:zhoRegistry , lang:polRegistry , lang:engRegistry , lang:deuRegistry , lang:jpnRegistry , lang:rusRegistry , lang:msaRegistry , lang:fraRegistry , lang:sweRegistry , lang:korRegistry ;
        geolexica:hasReview    reviews:Review954 .

