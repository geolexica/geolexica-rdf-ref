:expected_risk  a              skos:Concept , owl:NamedIndividual ;
        rdfs:label             "expected risk" ;
        dcterms:identifier     1219 ;
        skos:definition        "예정된 가치(통계적) 손실"@ko , "ожидаемая величина (статистика) потерь"@ru , "valor (estadístico) esperado de pérdidas"@es , "expected value (statistics) of loss"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1219 ;
        skos:prefLabel         "예상 위험"@ko , "risiko jangkaan"@ms , "ожидаемый риск"@ru , "expected risk"@en , "riesgo esperado"@es ;
        lang:eng-note          "Note to entry: Expected risk is calculated by multiplying the probability of the types of infringement by the cost of that infringement, summed up over all types of infringement."@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:msa-note          notes:NoteTransUnavail , notes:DefnTransUnavail ;
        lang:rus-note          "Примечание: Ожидаемый риск рассчитывается путём умножения вероятности возникновения типов нарушений на стоимость данного нарушения, суммируемого по всем типам нарушений."@ru ;
        lang:spa-note          "Nota: El riesgo esperado se calcula multiplicando la probabilidad de los tipos de vulneración por el coste de esa vulneración, y sumando el total para todos los tipos de vulneración."@es ;
        geolexica:hasInfo      info:Info1219 ;
        geolexica:hasLDAPI     ldapi:ldapi1219-rdfxml , ldapi:ldapi1219-skosrdf , ldapi:ldapi1219-json , ldapi:ldapi1219-jsonld ;
        geolexica:hasLangCode  lang:swe , lang:rus , lang:msa , lang:jpn , lang:kor , lang:deu , lang:zho , lang:pol , lang:ara , lang:fin , lang:eng , lang:fra , lang:spa , lang:nld ;
        geolexica:hasOrigin    lang:jpnRegistry , lang:nldRegistry , lang:sweRegistry , lang:polRegistry , lang:spaRegistry , lang:engRegistry , lang:rusRegistry , lang:araRegistry , lang:deuRegistry , lang:korRegistry , lang:fraRegistry , lang:msaRegistry , lang:finRegistry , lang:zhoRegistry .

