:binding  a                    owl:NamedIndividual , skos:Concept ;
        rdfs:label             "binding" ;
        dcterms:identifier     2110 ;
        skos:definition        "especificación de una correspondencia que relaciona la información definida en un modelo de contenido contenidos (datos y metadatos) con el formato de datos que lleva esa información"@es , "specification of a mapping relating the information defined in a content model (data and metadata) to the data format that carries that information"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2110 ;
        skos:prefLabel         "binding"@en , "vínculo"@es ;
        geolexica:hasInfo      info:Info2110 ;
        geolexica:hasLDAPI     ldapi:ldapi2110-rdfxml , ldapi:ldapi2110-jsonld , ldapi:ldapi2110-skosrdf , ldapi:ldapi2110-json ;
        geolexica:hasLangCode  lang:jpn , lang:msa , lang:spa , lang:zho , lang:deu , lang:eng , lang:nld , lang:fin , lang:fra , lang:kor , lang:rus , lang:pol , lang:swe , lang:ara ;
        geolexica:hasOrigin    lang:engRegistry , lang:fraRegistry , lang:araRegistry , lang:zhoRegistry , lang:spaRegistry , lang:msaRegistry , lang:deuRegistry , lang:finRegistry , lang:polRegistry , lang:rusRegistry , lang:nldRegistry , lang:korRegistry , lang:jpnRegistry , lang:sweRegistry ;
        geolexica:hasReview    reviews:Review2110 .

