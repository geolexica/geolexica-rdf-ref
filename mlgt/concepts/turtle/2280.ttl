###########################################################

:representation_information
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "representation information" ;
        dcterms:identifier     2280 ;
        skos:definition        "information that maps a data object into more meaningful concepts"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2280 ;
        skos:prefLabel         "representation information"@en ;
        geolexica:examples     "EXAMPLE 1 Representation information for a bit sequence which is a FITS file might consist of the FITS standard which defines the format plus a dictionary which defines the meaning in the file of keywords which are not part of the standard."@en , "EXAMPLE 2 JPEG software which is used to render a JPEG file; rendering the JPEG file as bits is not very meaningful to humans but the software, which embodies an understanding of the JPEG standard, maps the bits into pixels which can then be rendered as an image for human viewing."@en ;
        geolexica:hasInfo      info:Info2280 ;
        geolexica:hasLDAPI     ldapi:ldapi2280-rdfxml , ldapi:ldapi2280-jsonld , ldapi:ldapi2280-skosrdf , ldapi:ldapi2280-json ;
        geolexica:hasLangCode  lang:kor , lang:deu , lang:spa , lang:rus , lang:zho , lang:eng , lang:fin , lang:swe , lang:pol , lang:nld , lang:msa , lang:ara , lang:jpn , lang:fra ;
        geolexica:hasOrigin    lang:fraRegistry , lang:araRegistry , lang:msaRegistry , lang:sweRegistry , lang:deuRegistry , lang:jpnRegistry , lang:finRegistry , lang:rusRegistry , lang:engRegistry , lang:nldRegistry , lang:zhoRegistry , lang:polRegistry , lang:korRegistry , lang:spaRegistry ;
        geolexica:hasReview    reviews:Review2280 .

