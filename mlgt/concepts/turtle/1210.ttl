:authentication  a             skos:Concept , owl:NamedIndividual ;
        rdfs:label             "authentication" ;
        dcterms:identifier     1210 ;
        skos:definition        "подтверждение того, что потенциальный партнер в разговоре способен представлять человека или организацию"@ru , "회담 상대인 잠재적 동반자가 개인 또는 기관을 대표할 역량이 있는지를 검증하는 것"@ko , "verificación de que un participante  está autorizado para representar a una persona u organización a través de una conversación"@es , "verification that a potential partner in a conversation is capable of representing a person or organization"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1210 ;
        skos:prefLabel         "authentication"@en , "аутентификация"@ru , "autenticación"@es , "인증"@ko ;
        geolexica:hasInfo      info:Info1210 ;
        geolexica:hasLDAPI     ldapi:ldapi1210-jsonld , ldapi:ldapi1210-rdfxml , ldapi:ldapi1210-json , ldapi:ldapi1210-skosrdf ;
        geolexica:hasLangCode  lang:spa , lang:swe , lang:jpn , lang:fra , lang:zho , lang:msa , lang:pol , lang:ara , lang:nld , lang:fin , lang:eng , lang:kor , lang:rus , lang:deu ;
        geolexica:hasOrigin    lang:rusRegistry , lang:spaRegistry , lang:fraRegistry , lang:jpnRegistry , lang:engRegistry , lang:polRegistry , lang:msaRegistry , lang:sweRegistry , lang:zhoRegistry , lang:deuRegistry , lang:korRegistry , lang:nldRegistry , lang:finRegistry , lang:araRegistry .

