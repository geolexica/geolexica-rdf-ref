:coverage_geometry  a          skos:Concept , owl:NamedIndividual ;
        rdfs:label             "coverage geometry" ;
        dcterms:identifier     100 ;
        skos:definition        "configuración del dominio de una cobertura en términos de coordenadas"@es , "تكوين نطاق التغطية الموصوف من خلال الإحداثيات"@ar , "좌표로 기술된 커버리지의 시공간적 영역에 대한 구성"@ko , "конфигурация области определения покрытия, описываемая в терминах координат"@ru , "koordinatbaserad beskrivning av den domän som definieras av ett domänobjekt"@sv , "konfiguracja dziedziny pokrycia opisana współrzędnymi"@pl , "configuration du domaine d&#39;une couverture décrite en termes de coordonnées"@fr , "configuration of the domain of a coverage described in terms of coordinates"@en , "konfiguration af domænet for en coverage beskrevet i form af koordinater"@da , "按照坐标描述的数据覆盖的域结构。"@zh , "koordinaattien avulla ilmaistu paikkatietojatkumon kokoonpano"@fi ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          100 ;
        skos:prefLabel         "геометрия покрытия"@ru , "geometria pokrycia"@pl , "覆盖几何"@zh , "Coveragegeometrie"@de , "هندسة التغطية"@ar , "커버리지 기하"@ko , "géométrie de couverture"@fr , "coverage geometry"@en , "coverage-geometri"@da , "paikkatietojatkumon geometria"@fi , "domängeometri"@sv , "geometría de la cobertura"@es ;
        lang:deu-note          notes:DefnTransUnavail ;
        lang:fra-note          notes:NoteTransDiff ;
        geolexica:hasInfo      info:Info100 ;
        geolexica:hasLDAPI     ldapi:ldapi100-jsonld , ldapi:ldapi100-json , ldapi:ldapi100-skosrdf , ldapi:ldapi100-rdfxml ;
        geolexica:hasLangCode  lang:deu , lang:fra , lang:eng , lang:rus , lang:spa , lang:swe , lang:dan , lang:zho , lang:fin , lang:kor , lang:msa , lang:ara ;
        geolexica:hasOrigin    lang:deuRegistry , lang:korRegistry , lang:engRegistry , lang:zhoRegistry , lang:finRegistry , lang:spaRegistry , lang:fraRegistry , lang:rusRegistry , lang:msaRegistry , lang:araRegistry , lang:sweRegistry , lang:danRegistry .

