:packaging_information
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "packaging information" ;
        dcterms:identifier     2275 ;
        skos:definition        "information used to bind and identify the components of an information package"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2275 ;
        skos:prefLabel         "packaging information"@en ;
        geolexica:examples     "EXAMPLE The ISO 9660 volume and directory information is used on a CD-ROM to provide the content of several files containing content information and preservation description information."@en ;
        geolexica:hasInfo      info:Info2275 ;
        geolexica:hasLDAPI     ldapi:ldapi2275-json , ldapi:ldapi2275-rdfxml , ldapi:ldapi2275-jsonld , ldapi:ldapi2275-skosrdf ;
        geolexica:hasLangCode  lang:spa , lang:ara , lang:deu , lang:pol , lang:jpn , lang:rus , lang:fra , lang:swe , lang:msa , lang:nld , lang:kor , lang:eng , lang:fin , lang:zho ;
