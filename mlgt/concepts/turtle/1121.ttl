:external_coord_ref_system
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "external coordinate reference system" ;
        dcterms:identifier     1121 ;
        skos:definition        "система координат, чьи исходные даты не зависят от объекта, чье местоположение описывается этой системой координат"@ru , "نظام إحداثيات مرجعي والذي مرجع إسناده مستقل عن الكائن الذي تم تحديده بواسطته"@ar , "sistema de referencia de coordenadas cuyo datum es independiente del objeto que se localiza con él"@es , "Koordinatenreferenzsystem, dessen Datum von darzustellenden Objekt unabhängig ist"@de , "coordinate reference system whose datum is independent of the object that is located by it"@en , "기준좌표체계에 의해 위치가 설정된 개체와 자료가 서로 독립적인 좌표 기준 체계"@ko , "koordinatreferenssystem vars datum är oberoende av de objekt som det lägesbestämmer"@sv ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1121 ;
        skos:prefLabel         "sistema de referencia de coordenadas externo"@es , "внешняя система координат"@ru , "외부 좌표 기준 체계"@ko , "Objektkoordinatenreferenzsystem"@de , "نظام إحداثيات مرجعي خارجي"@ar , "externt koordinatreferenssystem"@sv , "external coordinate reference system"@en ;
        geolexica:hasInfo      info:Info1121 ;
        geolexica:hasLDAPI     ldapi:ldapi1121-skosrdf , ldapi:ldapi1121-jsonld , ldapi:ldapi1121-json , ldapi:ldapi1121-rdfxml ;
        geolexica:hasLangCode  lang:spa , lang:ara , lang:msa , lang:nld , lang:pol , lang:fin , lang:kor , lang:jpn , lang:deu , lang:eng , lang:zho , lang:fra , lang:rus , lang:swe ;
        geolexica:hasOrigin    lang:polRegistry , lang:deuRegistry , lang:zhoRegistry , lang:msaRegistry , lang:rusRegistry , lang:spaRegistry , lang:finRegistry , lang:jpnRegistry , lang:korRegistry , lang:sweRegistry , lang:fraRegistry , lang:engRegistry , lang:nldRegistry , lang:araRegistry ;
        geolexica:hasReview    reviews:Review1121 .

