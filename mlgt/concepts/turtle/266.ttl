        skos:notation          266 ;
        skos:prefLabel         "идентификатор языка"@ru , "معرِّف لغة"@ar , "identificador de lenguaje"@es , "언어 식별자"@ko , "language identifier"@en , "language identifier"@sv , "Sprachidentifikator"@de ;
        lang:lang-deu          notes:DefnTransUnavail ;
        geolexica:hasInfo      info:Info266 ;
        geolexica:hasLDAPI     ldapi:ldapi266-skosrdf , ldapi:ldapi266-json , ldapi:ldapi266-rdfxml , ldapi:ldapi266-jsonld ;
        geolexica:hasLangCode  lang:rus , lang:eng , lang:swe , lang:ara , lang:deu , lang:spa , lang:kor ;
        geolexica:hasOrigin    lang:rusRegistry , lang:korRegistry , lang:spaRegistry , lang:sweRegistry , lang:araRegistry , lang:deuRegistry , lang:engRegistry ;
