:stray_light  a                skos:Concept , owl:NamedIndividual ;
        rdfs:label             "stray light" ;
        dcterms:identifier     1609 ;
        skos:definition        "radiación electromagnética que ha sido detectada pero no proviene directamente del IFOV"@es , "электромагнитное излучение, которое было обнаружено, но не попало непосредственно из IFOV"@ru , "electromagnetic radiation that has been detected but did not come directly from the IFOV"@en , "radiasi elektromagnet yang dikesan tetapi tidak datang daripada IFOV"@ms , "IFOV에서 감지되었으나 직접 전달되진 않은 전자기 방사선"@ko ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1609 ;
        skos:prefLabel         "미광"@ko , "stray light"@en , "luz parásita"@es , "cahaya lilau"@ms , "рассеянный свет"@ru ;
        lang:eng-note          "Note 2 to entry: This definition is valid for the optical portion of the spectrum under observation."@en , "Note 1 to entry: Stray light may be reflected light within a telescope."@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:msa-note          notes:NoteTransUnavail ;
        lang:rus-note          "Примечание 2: это определение применимо к оптической части наблюдаемого спектра."@ru , "Примечание 1: рассеянный свет может быть отраженным светом в телескопе."@ru ;
        geolexica:hasInfo      info:Info1609 ;
        geolexica:hasLDAPI     ldapi:ldapi1609-json , ldapi:ldapi1609-rdfxml , ldapi:ldapi1609-skosrdf , ldapi:ldapi1609-jsonld ;
        geolexica:hasLangCode  lang:nld , lang:spa , lang:fra , lang:fin , lang:rus , lang:zho , lang:kor , lang:jpn , lang:ara , lang:deu , lang:pol , lang:swe , lang:msa , lang:eng ;
        geolexica:hasOrigin    lang:sweRegistry , lang:spaRegistry , lang:korRegistry , lang:msaRegistry , lang:finRegistry , lang:zhoRegistry , lang:nldRegistry , lang:araRegistry , lang:rusRegistry , lang:deuRegistry , lang:fraRegistry , lang:engRegistry , lang:polRegistry , lang:jpnRegistry .

