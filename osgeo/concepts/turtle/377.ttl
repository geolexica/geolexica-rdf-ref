:c377   a                     skos:Concept , owl:NamedIndividual ;
        rdfs:label            "Spatial Data Infrastructure" ;
        dcterms:identifier    "377"^^xsd:integer ;
        skos:definition       "computer system integrated by a set of resources to manage geographic information available from the Internet, that follow a set of interoperational capacity conditions that allow access through a simple browser and can be combined based on user needs"@en ;
        skos:inScheme         osgeo:OSGeoConceptScheme ;
        skos:notation         "377"^^xsd:integer ;
        skos:prefLabel        "Spatial Data Infrastructure"@en ;
        osgeo:abbreviation    "SDI" ;
        osgeo:classification  "preferred"@en ;
        osgeo:example         "EXAMPLE 2  Rules, specifications, protocols, and interfaces are examples of interoperational capacity conditions."@en , "EXAMPLE 1  Catalogues, servers, programs, data, applications, web pages are examples of various resources."@en ;
        osgeo:hasInfo         osgeo-info:Info377 ;
