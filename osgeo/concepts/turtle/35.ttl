:c35    a                     skos:Concept , owl:NamedIndividual ;
        rdfs:label            "catalogue service" ;
        dcterms:identifier    "35"^^xsd:integer ;
        skos:definition       "allows the publication and searching of information (metadata) describing data (cartography), services, applications and any other kind of resources. The catalogue services are necessary to supply search and invocation capabilities over registered resources within an SDI. A Open Geospatial Consortium spec establishes how a Catalogue Service must be standard and interoperable."@en ;
        skos:inScheme         osgeo:OSGeoConceptScheme ;
        skos:notation         "35"^^xsd:integer ;
        skos:prefLabel        "catalogue service"@en ;
        osgeo:classification  "preferred"@en ;
        osgeo:hasInfo         osgeo-info:Info35 ;
        osgeo:hasLDAPI        osgeo-ldapi:ldapi35-json , osgeo-ldapi:ldapi35-skosrdf , osgeo-ldapi:ldapi35-rdfxml , osgeo-ldapi:ldapi35-jsonld ;
        osgeo:hasOrigin       osgeo-lang:engRegistry , "https://osgeo.geolexica.org/registers/#language-eng"^^xsd:anyURI ;
        osgeo:hasReview       osgeo-review:Review35 ;
        osgeo:jsonURI         "https://osgeo.geolexica.org/api/ldapi/35.json"^^xsd:anyURI ;
        osgeo:jsonldURI       "https://osgeo.geolexica.org/api/ldapi/35.jsonld"^^xsd:anyURI ;
        osgeo:rdfxmlURI       "https://osgeo.geolexica.org/api/ldapi/35.rdf"^^xsd:anyURI ;
