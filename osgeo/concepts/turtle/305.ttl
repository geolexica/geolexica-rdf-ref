:c305   a                     skos:Concept , owl:NamedIndividual ;
        rdfs:label            "OGC Web Map Service Interface Standard" ;
        dcterms:identifier    "305"^^xsd:integer ;
        skos:definition       "OGC standard that describes how to systematically produce rendered map images from a service and return them to a client"@en ;
        skos:inScheme         osgeo:OSGeoConceptScheme ;
        skos:notation         "305"^^xsd:integer ;
        skos:prefLabel        "OGC Web Map Service Interface Standard"@en ;
        osgeo:abbreviation    "WMS" ;
        osgeo:classification  "preferred"@en ;
        osgeo:hasInfo         osgeo-info:Info305 ;
        osgeo:hasLDAPI        osgeo-ldapi:ldapi305-skosrdf , osgeo-ldapi:ldapi305-jsonld , osgeo-ldapi:ldapi305-rdfxml , osgeo-ldapi:ldapi305-json ;
        osgeo:hasOrigin       osgeo-lang:engRegistry , "https://osgeo.geolexica.org/registers/#language-eng"^^xsd:anyURI ;
        osgeo:hasReview       osgeo-review:Review305 ;
        osgeo:hasSource       "https://mapserver.org/glossary.html" ;
        osgeo:jsonURI         "https://osgeo.geolexica.org/api/ldapi/305.json"^^xsd:anyURI ;
        osgeo:jsonldURI       "https://osgeo.geolexica.org/api/ldapi/305.jsonld"^^xsd:anyURI ;
        osgeo:rdfxmlURI       "https://osgeo.geolexica.org/api/ldapi/305.rdf"^^xsd:anyURI ;
        osgeo:skosrdfURI      "https://osgeo.geolexica.org/api/ldapi/305.ttl"^^xsd:anyURI ;
        osgeo-lang:eng-note   "Note to entry:  The Web Map Service is a standard protocol for serving georeferenced map images over the Internet that are generated by a map server using data from a GIS database. The specification was developed and first published by the Open Geospatial Consortium in 1999."@en .

