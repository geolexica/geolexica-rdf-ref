	<identifier xmlns="http://purl.org/dc/terms/" rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">307</identifier>
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
