        dcterms:modified "2022-01-26"^^xsd:date ;
        dcterms:publisher <https://www.osgeo.org/> ;
        dcterms:title "Notes RDF Dataset (MLGT)"@en ;
        rdfs:label "Notes RDF Dataset (MLGT)" ;
        vann:preferredNamespacePrefix "notes" ;
        vann:preferredNamespaceUri "https://isotc211.geolexica.org/api/ld/notes#" .

dctype:Dataset owl:sameAs void:Dataset .

