    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#MLGTGlossary">
    <skos:note xml:lang="en">The MLGT Glossary (SKOS) Collection may not be necessary and is subject to change. It is being used to differentiate the MLGT set of SKOS concepts from the OSGeo set of SKOS concepts. Rationale is that the Geolexica Ontology can be used for either collection and one may want to identify concepts as such.</skos:note>
    <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://isotc211.geolexica.org/</rdfs:seeAlso>
    <rdfs:label xml:lang="en">MLGT Collection of SKOS Concepts</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Collection"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#ConceptReview">
    <skos:prefLabel xml:lang="en">Review</skos:prefLabel>
    <rdfs:label xml:lang="en">Review</rdfs:label>
    <rdfs:comment xml:lang="en">Attributes values for each concept/term in the glossary, such as "last review performed", "status", "decision", decision event", and "notes". New attributes may be included in future versions of the ontology.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#classification">
    <skos:note xml:lang="en">Need to identify and enumerate classification values (i.e., "preferred", "alternate"...).</skos:note>
    <skos:note xml:lang="en">Is there a vocabulary with this property (reuse)?</skos:note>
    <rdfs:label xml:lang="en">classification</rdfs:label>
    <dcterms:description xml:lang="en">Classification of the term pertaining to concept INFO.</dcterms:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="https://osgeo.geolexica.org/api/geolexica#ConceptInfo"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status">
    <skos:note xml:lang="en">Need to identify and enumerate status values (i.e., "accepted", "final", "pending"...).</skos:note>
    <skos:note xml:lang="en">Attribute is drawn from the SemWeb Vocab Status ontology.</skos:note>
    <rdfs:label xml:lang="en">Status</rdfs:label>
    <dcterms:description xml:lang="en">Status of a vocabulary term for Concept Info and/or Concept Review.</dcterms:description>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="https://osgeo.geolexica.org/api/geolexica#ConceptReview"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#GeolexicaConceptScheme">
    <skos:prefLabel xml:lang="en">Geolexica Concept Scheme</skos:prefLabel>
    <rdfs:label xml:lang="en">Geolexica Concept Scheme</rdfs:label>
    <dcterms:description xml:lang="en">In accordance with SKOS, this is the Geolexica Concept Scheme, to which all SKOS Concepts are attributed in the OSGeo Geolexica Glossary and/or ISO/TC 211 Multi-lingual Glossary of Terms (MLGT).</dcterms:description>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/issued">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#OSGeoGlossary">
    <skos:note xml:lang="en">The OSGeo Glossary (SKOS) Collection may not be necessary and is subject to change. It is being used to differentiate the OSGeo set of SKOS concepts from the MLGT set of SKOS concepts. Rationale is that the Geolexica Ontology can be used for either collection and one may want to identify concepts as such.</skos:note>
    <skos:note xml:lang="en">OSGeo Collection of SKOS Concepts</skos:note>
    <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://osgeo.geolexica.org/</rdfs:seeAlso>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Collection"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#ConceptInfo">
    <skos:prefLabel xml:lang="en">Info</skos:prefLabel>
    <rdfs:label xml:lang="en">Information</rdfs:label>
    <rdfs:comment xml:lang="en">Attribute values for each concept/term in the glossary that indicates "status", "classification", and "date-accepted". New attributes may be included in future versions of the ontology.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/license">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/title">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AnnotationProperty"/>
