    <rdfs:label xml:lang="en">Linked Data API</rdfs:label>
    <dcterms:description xml:lang="en">Herein lies classes for linked data APIs, otherwise considered distinct data format representations of each concept in the glossary.</dcterms:description>
    <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/MediaType"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://osgeo.geolexica.org/api/geolexica#jsonURI">
