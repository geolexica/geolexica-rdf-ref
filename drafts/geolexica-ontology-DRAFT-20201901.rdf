    <dcterms:license>Copyright 2022 OSGeo</dcterms:license>
    <dcterms:description xml:lang="en">This is the core ontology for OSGeo Geolexica, the online glossary for OSGeo.</dcterms:description>
    <dcterms:publisher rdf:resource="https://www.osgeo.org/"/>
    <skos:note xml:lang="en">The Geolexica Ontology is currently under development as of 2022-19-01. Classes, properties, attributes, and axioms are subject to change.</skos:note>
    <dcterms:created>2022-01-12</dcterms:created>
    <vann:preferredNamespacePrefix>geolexica</vann:preferredNamespacePrefix>
    <owl:versionInfo>Version 0.1.0</owl:versionInfo>
    <vann:preferredNamespaceUri>https://osgeo.geolexica.org/api/geolexica#</vann:preferredNamespaceUri>
    <dcterms:title xml:lang="en">OSGeo Geolexica Ontology</dcterms:title>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.linkedmodel.org/schema/vaem#abbreviation">
    <skos:note xml:lang="en">Attribute is drawn from the Vocabulary for Essential Metadata.</skos:note>
