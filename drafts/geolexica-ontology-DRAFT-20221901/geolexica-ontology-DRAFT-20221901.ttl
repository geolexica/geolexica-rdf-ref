@prefix : <https://osgeo.geolexica.org/api/geolexica#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vaem: <http://www.linkedmodel.org/schema/vaem#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix void: <http://vocab.deri.ie/void#> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <https://osgeo.geolexica.org/api/geolexica> .

<https://osgeo.geolexica.org/api/geolexica> rdf:type owl:Ontology ;
                                             owl:versionIRI <https://osgeo.geolexica.org/api/geolexica/v0.1.0/> ;
                                             owl:imports <http://www.w3.org/2004/02/skos/core> ;
                                             dcterms:contributor <https://www.linkedin.com/in/rhtse/> ;
                                             dcterms:created "2022-01-12" ;
                                             dcterms:creator <https://www.linkedin.com/in/lanemk> ;
                                             dcterms:description "This is the core ontology for OSGeo Geolexica, the online glossary for OSGeo."@en ;
                                             dcterms:issued "" ;
                                             dcterms:license "Copyright 2022 OSGeo" ;
                                             dcterms:modified "" ;
                                             dcterms:publisher <https://www.osgeo.org/> ;
                                             dcterms:title "OSGeo Geolexica Ontology"@en ;
                                             vann:preferredNamespacePrefix "geolexica" ;
                                             vann:preferredNamespaceUri "https://osgeo.geolexica.org/api/geolexica#" ;
                                             owl:versionInfo "Version 0.1.0" ;
                                             skos:note "The Geolexica Ontology is currently under development as of 2022-19-01. Classes, properties, attributes, and axioms are subject to change."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dcterms:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
dcterms:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/dateAccepted
dcterms:dateAccepted rdf:type owl:DatatypeProperty ;
                     dcterms:description "Date the concept/term was accepted in concept INFO."@en ;
                     rdfs:label "date accepted"@en ;
                     skos:note "Attribute is drawn from the DCMI Metadata Terms vocabulary."@en .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:DatatypeProperty ;
                   rdfs:domain skos:Concept ;
                   rdfs:range xsd:integer ;
                   dcterms:description "Encodes the Term ID for each concept."@en ;
                   rdfs:comment "Use for Term ID for each concept in the glossary; must be an integer."@en ;
                   rdfs:label "identifier"@en ;
                   skos:note "Attribute is drawn from the DCMI Metadata Terms vocabulary."@en .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:DatatypeProperty .


###  http://www.linkedmodel.org/schema/vaem#abbreviation
vaem:abbreviation rdf:type owl:DatatypeProperty ;
                  rdfs:domain skos:Concept ;
                  rdfs:range xsd:string ;
                  dcterms:description "ISO 3166 three-letter language codes assigned to each translation provided for a given concept."@en ;
                  rdfs:label "Abbreviation"@en ;
                  skos:note "Attribute is drawn from the Vocabulary for Essential Metadata."@en .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
vs:term_status rdf:type owl:DatatypeProperty ;
               rdfs:domain :ConceptReview ;
               rdfs:range xsd:string ;
               dcterms:description "Status of a vocabulary term for Concept Info and/or Concept Review."@en ;
               rdfs:label "Status"@en ;
               skos:note "Attribute is drawn from the SemWeb Vocab Status ontology."@en ,
                         "Need to identify and enumerate status values (i.e., \"accepted\", \"final\", \"pending\"...)."@en .


###  https://osgeo.geolexica.org/api/geolexica#classification
:classification rdf:type owl:DatatypeProperty ;
                rdfs:domain :ConceptInfo ;
                rdfs:range xsd:string ;
                dcterms:description "Classification of the term pertaining to concept INFO."@en ;
                rdfs:label "classification"@en ;
                skos:note "Is there a vocabulary with this property (reuse)?"@en ,
                          "Need to identify and enumerate classification values (i.e., \"preferred\", \"alternate\"...)."@en .


###  https://osgeo.geolexica.org/api/geolexica#decision
:decision rdf:type owl:DatatypeProperty ;
          rdfs:domain :ConceptReview ;
          rdfs:range xsd:string ;
          dcterms:description "Disposition of concept/term REVIEW."@en ;
          rdfs:label "decision"@en ;
          skos:note "Is there a vocabulary with this property (reuse)?"@en ,
                    "Need to identify and enumerate decision values (i.e., \"accepted\", \"rejected\", \"pending\"...)."@en .


###  https://osgeo.geolexica.org/api/geolexica#decisionEvent
:decisionEvent rdf:type owl:DatatypeProperty ;
               rdfs:domain :ConceptReview ;
               rdfs:range xsd:string ;
               dcterms:description "Context statement of authority REVIEW."@en ;
               rdfs:label "decision event"@en ;
               skos:note "Is there a vocabulary with this property (reuse)?"@en .


###  https://osgeo.geolexica.org/api/geolexica#jsonURI
:jsonURI rdf:type owl:DatatypeProperty ;
         rdfs:domain :JSON ;
         rdfs:range xsd:anyURI ;
         dcterms:description "JSON  property representation of resource."@en ;
         rdfs:label "JSON API URI"@en ;
         skos:note "Property value is URI link to file."@en .


###  https://osgeo.geolexica.org/api/geolexica#jsonldURI
:jsonldURI rdf:type owl:DatatypeProperty ;
           rdfs:domain :JSON-LD ;
           rdfs:range xsd:anyURI ;
           dcterms:description "JSON-LD property representation of resource."@en ;
           rdfs:label "JSON-LD API URI"@en ;
           skos:note "Property value is URI link to file."@en .


###  https://osgeo.geolexica.org/api/geolexica#lastReviewPerformed
:lastReviewPerformed rdf:type owl:DatatypeProperty ;
                     rdfs:domain :ConceptReview ;
                     rdfs:range xsd:date ;
                     dcterms:description "Date of the last REVIEW performed for the concept/term."@en ;
                     rdfs:label "last review performed"@en ;
                     skos:note "Is there a vocabulary with this property (reuse)?"@en .


###  https://osgeo.geolexica.org/api/geolexica#notes
:notes rdf:type owl:DatatypeProperty ;
       rdfs:domain :ConceptReview ;
       rdfs:range xsd:string ;
       dcterms:description "Notes pertaining to concept/term REVIEW."@en ;
       rdfs:label "notes"@en ;
       skos:note "Is there a vocabulary with this property (reuse)?"@en .


###  https://osgeo.geolexica.org/api/geolexica#origin
:origin rdf:type owl:DatatypeProperty ;
        rdfs:domain skos:Concept ;
        rdfs:range xsd:string ;
        dcterms:description "Origin of defintion in ISO-TC 211 Registers"@en ;
        rdfs:label "origin"@en ;
        skos:note "Is there a vocabulary with this property (reuse)?"@en .


###  https://osgeo.geolexica.org/api/geolexica#skosrdfURI
:skosrdfURI rdf:type owl:DatatypeProperty ;
            rdfs:domain :SKOS-RDF ;
            rdfs:range xsd:anyURI ;
            dcterms:description "SKOS-RDF property representation of a resource."@en ;
            rdfs:label "SKOS-RDF API URI"@en ;
            skos:altLabel "SKOS IN RDF API URI"@en ;
            skos:note "Property value is URI link to file."@en .


###  https://osgeo.geolexica.org/api/geolexica#source
:source rdf:type owl:DatatypeProperty ;
        rdfs:domain skos:Concept ;
        rdfs:range xsd:string ;
        dcterms:description "Source of concept/term defintion."@en ;
        rdfs:label "source"@en ;
        skos:note "Is there a vocabulary with this property (reuse)?"@en .


###  https://osgeo.geolexica.org/api/geolexica#tbxisoxmlURI
:tbxisoxmlURI rdf:type owl:DatatypeProperty ;
              rdfs:domain :TBXISOXML ;
              rdfs:range xsd:anyURI ;
              dcterms:description "TBX-ISO-XML property representation of a resource."@en ;
              rdfs:label "TBX ISO XML API URI"@en ;
              skos:note "Property value is URI link to file."@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/MediaType
dcterms:MediaType rdf:type owl:Class .


###  https://osgeo.geolexica.org/api/geolexica#ConceptInfo
:ConceptInfo rdf:type owl:Class ;
             rdfs:comment "Attribute values for each concept/term in the glossary that indicates \"status\", \"classification\", and \"date-accepted\". New attributes may be included in future versions of the ontology."@en ;
             rdfs:label "Information"@en ;
             skos:prefLabel "Info"@en .


###  https://osgeo.geolexica.org/api/geolexica#ConceptReview
:ConceptReview rdf:type owl:Class ;
               rdfs:comment "Attributes values for each concept/term in the glossary, such as \"last review performed\", \"status\", \"decision\", decision event\", and \"notes\". New attributes may be included in future versions of the ontology."@en ;
               rdfs:label "Review"@en ;
               skos:prefLabel "Review"@en .


###  https://osgeo.geolexica.org/api/geolexica#JSON
:JSON rdf:type owl:Class ;
      rdfs:subClassOf :LinkedDataAPI ;
      dcterms:description "JSON class representation of a resource."@en ;
      rdfs:label "JSON Linked Data API"@en .


###  https://osgeo.geolexica.org/api/geolexica#JSON-LD
:JSON-LD rdf:type owl:Class ;
         rdfs:subClassOf :LinkedDataAPI ;
         dcterms:description "JSON-LD class representation of a resource."@en ;
         rdfs:label "JSON-LD Linked Data API"@en .


###  https://osgeo.geolexica.org/api/geolexica#LinkedDataAPI
:LinkedDataAPI rdf:type owl:Class ;
               rdfs:subClassOf dcterms:MediaType ;
               dcterms:description "Herein lies classes for linked data APIs, otherwise considered distinct data format representations of each concept in the glossary."@en ;
               rdfs:label "Linked Data API"@en ;
               skos:prefLabel "Linked Data API"@en .


###  https://osgeo.geolexica.org/api/geolexica#Origin
:Origin rdf:type owl:Class ;
        dcterms:description "Source standards document for given language; may include location of chapter, paragraph, and section."@en ;
        rdfs:label "Origin"@en ;
        skos:note "Need to attach all known Registers, all standards document language references as INDIVIDUALS."@en ,
                  "Should be a URL linking to Registers document."@en .


###  https://osgeo.geolexica.org/api/geolexica#SKOS-RDF
:SKOS-RDF rdf:type owl:Class ;
          rdfs:subClassOf :LinkedDataAPI ;
          dcterms:description "SKOS-RDF class representation of a resource."@en ;
          rdfs:label "SKOS-RDF Linked Data API"@en ;
          skos:altLabel "SKOS IN RDF Linked Data API"@en .


###  https://osgeo.geolexica.org/api/geolexica#Source
:Source rdf:type owl:Class ;
        dcterms:description "Source standards document; may include location of chapter, paragraph, and section."@en ;
        rdfs:label "Source"@en ;
        skos:note "Need to attach all known Source documents as INDIVIDUALS."@en ,
                  "Should be a URL linking to standards document."@en .


###  https://osgeo.geolexica.org/api/geolexica#TBXISOXML
:TBXISOXML rdf:type owl:Class ;
           rdfs:subClassOf :LinkedDataAPI ;
           dcterms:description "TBX-ISO-XML class representation of a resource."@en ;
           rdfs:label "TBX-ISO-XML Linked Data API"@en ;
           skos:note "Is this supposed to be TBX-ISO-XML or -TML?"@en .


#################################################################
#    Individuals
#################################################################

###  https://osgeo.geolexica.org/api/geolexica#GeolexicaConceptScheme
:GeolexicaConceptScheme rdf:type owl:NamedIndividual ,
                                 skos:ConceptScheme ;
                        dcterms:description "In accordance with SKOS, this is the Geolexica Concept Scheme, to which all SKOS Concepts are attributed in the OSGeo Geolexica Glossary and/or ISO/TC 211 Multi-lingual Glossary of Terms (MLGT)."@en ;
                        rdfs:label "Geolexica Concept Scheme"@en ;
                        skos:prefLabel "Geolexica Concept Scheme"@en .


###  https://osgeo.geolexica.org/api/geolexica#MLGTGlossary
:MLGTGlossary rdf:type owl:NamedIndividual ,
                       skos:Collection ;
              rdfs:label "MLGT Collection of SKOS Concepts"@en ;
              rdfs:seeAlso "https://isotc211.geolexica.org/"^^xsd:anyURI ;
              skos:note "The MLGT Glossary (SKOS) Collection may not be necessary and is subject to change. It is being used to differentiate the MLGT set of SKOS concepts from the OSGeo set of SKOS concepts. Rationale is that the Geolexica Ontology can be used for either collection and one may want to identify concepts as such."@en .


###  https://osgeo.geolexica.org/api/geolexica#OSGeoGlossary
:OSGeoGlossary rdf:type owl:NamedIndividual ,
                        skos:Collection ;
               rdfs:seeAlso "https://osgeo.geolexica.org/"^^xsd:anyURI ;
               skos:note "OSGeo Collection of SKOS Concepts"@en ,
                         "The OSGeo Glossary (SKOS) Collection may not be necessary and is subject to change. It is being used to differentiate the OSGeo set of SKOS concepts from the MLGT set of SKOS concepts. Rationale is that the Geolexica Ontology can be used for either collection and one may want to identify concepts as such."@en .
