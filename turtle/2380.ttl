:transform_reference_epoch
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "transformation reference epoch" ;
        dcterms:identifier     2380 ;
        skos:definition        "epoch at which the parameter values of a time-specific coordinate transformation are valid"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2380 ;
        skos:prefLabel         "transformation reference epoch"@en ;
        lang:eng-note          "Note to entry: Coordinates first need to be propagated to this epoch before the coordinate transformation is applied. This is in contrast to a parameter reference epoch where the transformation parameter values first need to be propagated to the epoch of the coordinates before the coordinate transformation is applied."@en ;
        geolexica:hasInfo      info:Info2380 ;
        geolexica:hasLDAPI     ldapi:ldapi2380-jsonld , ldapi:ldapi2380-skosrdf , ldapi:ldapi2380-rdfxml , ldapi:ldapi2380-json ;
        geolexica:hasLangCode  lang:rus , lang:fin , lang:jpn , lang:eng , lang:nld , lang:fra , lang:ara , lang:msa , lang:pol , lang:spa , lang:kor , lang:deu , lang:zho , lang:swe ;
        geolexica:hasOrigin    lang:spaRegistry , lang:rusRegistry , lang:sweRegistry , lang:nldRegistry , lang:jpnRegistry , lang:polRegistry , lang:deuRegistry , lang:araRegistry , lang:engRegistry , lang:korRegistry , lang:zhoRegistry , lang:fraRegistry , lang:finRegistry , lang:msaRegistry ;
        geolexica:hasReview    reviews:Review2380 .

