:open_set  a                   owl:NamedIndividual , skos:Concept ;
        rdfs:label             "open set (metric, topology, geometry)" ;
        dcterms:identifier     2245 ;
        skos:definition        "containing a metric or topologically open neighborhood of each of its points"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2245 ;
        skos:prefLabel         "open set (metric, topology, geometry)"@en ;
        lang:eng-note          "Note to entry: In a metric space, a set X us open if each point x in the set is contained in some small ball which is a subset of X: [X \\text{ is open}] \\Leftrightarrow [x \\in X] \\implies [\\exists \\varepsilon > 0 \\backepsilon \\text{distance} (x,y) < \\varepsilon] \\implies [y \\in X]. A topology is a set of subsets of a space which are considered open."@en ;
        geolexica:hasInfo      info:Info2245 ;
        geolexica:hasLDAPI     ldapi:ldapi2245-jsonld , ldapi:ldapi2245-json , ldapi:ldapi2245-skosrdf , ldapi:ldapi2245-rdfxml ;
        geolexica:hasLangCode  lang:jpn , lang:swe , lang:rus , lang:deu , lang:ara , lang:msa , lang:nld , lang:eng , lang:spa , lang:pol , lang:zho , lang:kor , lang:fra , lang:fin ;
