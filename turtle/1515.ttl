:facility  a                   skos:Concept , owl:NamedIndividual ;
        rdfs:label             "facility" ;
        dcterms:identifier     1515 ;
        skos:definition        "construcción física o área física a la que se puede acceder y usar"@es , "physical installation or physical area that may be accessed and used"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1515 ;
        skos:prefLabel         "facility"@en , "instalación"@es ;
        lang:eng-note          "Note to entry: Such facilities may be available on-board transport means during the transport, at arrivals to and at departures from a transfer node, and at transfer nodes."@en ;
        lang:spa-note          "Nota: Dichas instalaciones pueden estar disponibles a bordo de los medios de transporte durante el transporte, en las llegadas y en las salidas de un nodo de conexión, y en los nodos de transbordo."@es ;
        geolexica:examples     "EXAMPLE Elevators, restaurant areas, waiting areas, seats, toilets, shops."@en , "EJEMPLO Ascensores, áreas de restaurantes, salas de espera, asientos, sanitarios tiendas."@es ;
        geolexica:hasInfo      info:Info1515 ;
        geolexica:hasLDAPI     ldapi:ldapi1515-jsonld , ldapi:ldapi1515-skosrdf , ldapi:ldapi1515-rdfxml , ldapi:ldapi1515-json ;
        geolexica:hasLangCode  lang:rus , lang:spa , lang:jpn , lang:msa , lang:nld , lang:fin , lang:ara , lang:fra , lang:kor , lang:zho , lang:pol , lang:swe , lang:deu , lang:eng ;
        geolexica:hasOrigin    lang:sweRegistry , lang:deuRegistry , lang:nldRegistry , lang:polRegistry , lang:jpnRegistry , lang:korRegistry , lang:zhoRegistry , lang:finRegistry , lang:fraRegistry , lang:rusRegistry , lang:araRegistry , lang:engRegistry , lang:spaRegistry , lang:msaRegistry ;
        geolexica:hasReview    reviews:Review1515 .

