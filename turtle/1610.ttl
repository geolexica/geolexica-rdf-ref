:smile_distortion  a           owl:NamedIndividual , skos:Concept ;
        rdfs:label             "smile distortion" ;
        dcterms:identifier     1610 ;
        skos:definition        "смещение центральной длины волны спектральных каналов, вызванное оптическим искажением"@ru , "광학적 왜곡 현상으로 인한 스펙트럼 채널의 중심 파장 변화"@ko , "variación del centro de longitud de onda de canales espectrales causada por distorsión óptica"@es , "centre wavelength shift of spectral channels caused by optical distortion"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1610 ;
        skos:prefLabel         "스마일 왜곡"@ko , "искажение в форме улыбки"@ru , "distorsión sonrisa"@es , "smile distortion"@en ;
        lang:eng-note          "Note to entry: This distortion is often simply called smile."@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:rus-note          "Примечание: Это искажение часто просто называют \"улыбкой\""@ru ;
        lang:spa-note          "Nota: Esta distorsión es en ocasiones llamada simplemente sonrisa"@es ;
        geolexica:hasInfo      info:Info1610 ;
        geolexica:hasLDAPI     ldapi:ldapi1610-skosrdf , ldapi:ldapi1610-jsonld , ldapi:ldapi1610-json , ldapi:ldapi1610-rdfxml ;
        geolexica:hasLangCode  lang:deu , lang:pol , lang:rus , lang:nld , lang:jpn , lang:eng , lang:swe , lang:spa , lang:fin , lang:zho , lang:fra , lang:ara , lang:kor , lang:msa ;
        geolexica:hasOrigin    lang:rusRegistry , lang:spaRegistry , lang:sweRegistry , lang:jpnRegistry , lang:msaRegistry , lang:fraRegistry , lang:araRegistry , lang:korRegistry , lang:deuRegistry , lang:finRegistry , lang:nldRegistry , lang:polRegistry , lang:zhoRegistry , lang:engRegistry .

