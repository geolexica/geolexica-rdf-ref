:definition  a                 owl:NamedIndividual , skos:Concept ;
        rdfs:label             "definition" ;
        dcterms:identifier     128 ;
        skos:definition        "연관 개념들과 차별화하기 위한 묘사적 기술을 통한 개념 표현"@ko , "beskrivning som ska avgränsa begreppet mot relaterade begrepp"@sv , "представление понятия описательным утверждением, которое служит для того, чтобы отличить его от связанных понятий"@ru , "beskrivelse af et begreb ved et deskriptivt udsagn, der tjener til at skelne det fra beslægtede begreber"@da , "توضيح لمفهوم بجملة وصفية لتمييزه عن  المفاهيم الاخرى ذات العلاقة"@ar , "representación de un concepto mediante un enunciado descriptivo que permite diferenciarlo de otros conceptos relacionados"@es , "representation of a concept by a descriptive statement which serves to differentiate it from related concepts"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          128 ;
        skos:prefLabel         "تعريف"@ar , "kejelasan"@ms , "Definition"@de , "definition"@sv , "definition"@en , "definition"@da , "정의"@ko , "определение"@ru , "definición"@es ;
        lang:deu-note          notes:DefnTransUnavail ;
        lang:msa-note          notes:DefnTransUnavail ;
        geolexica:hasInfo      info:Info128 ;
        geolexica:hasLDAPI     ldapi:ldapi128-jsonld , ldapi:ldapi128-json , ldapi:ldapi128-skosrdf , ldapi:ldapi128-rdfxml ;
        geolexica:hasLangCode  lang:rus , lang:kor , lang:eng , lang:deu , lang:msa , lang:dan , lang:spa , lang:ara , lang:swe ;
        geolexica:hasOrigin    lang:sweRegistry , lang:korRegistry , lang:rusRegistry , lang:araRegistry , lang:msaRegistry , lang:engRegistry , lang:danRegistry , lang:deuRegistry , lang:spaRegistry ;
        geolexica:hasReview    reviews:Review128 .

