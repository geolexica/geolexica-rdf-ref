:service_broker  a             owl:NamedIndividual , skos:Concept ;
        rdfs:label             "service broker" ;
        dcterms:identifier     683 ;
        skos:definition        "application combinant ou offrant des services de niveau inférieur pour des besoins utilisateur spécifiques"@fr , "특정 사용자 요구 사항에 대하여 하위 수준의 서비스를 결합하거나 제공하는 응용"@ko , "application that combines or offers lower-level services for specific user needs"@en , "为特定用户的需求提供或组合基本服务的应用。"@zh , "تطبيق يوحد أو يقدم مستوى أدنى من الخدمات بالنسبة لاحتياجات محددة للمستخدمين"@ar , "приложение,  которое комбинирует или предлагает сервисы  более низкого уровня  с учетом потребностей пользователя"@ru , "sovellus, joka yhdistää tai tarjoaa alemman tason palveluja käyttäjien tiettyihin tarpeisiin"@fi , "aplicación que combina u ofrece servicios de nivel inferior para las necesidades específicas de los usuarios"@es ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          683 ;
        skos:prefLabel         "服务代理"@zh , "courtier de services"@fr , "брокер сервISOв"@ru , "وسيط خدمة"@ar , "palvelunvälittäjä"@fi , "service broker"@en , "서비스 브로커"@ko , "intermediario de servicios"@es ;
        lang:fra-note          notes:NoteTransDiff ;
        geolexica:hasInfo      info:Info683 ;
        geolexica:hasLDAPI     ldapi:ldapi683-jsonld , ldapi:ldapi683-rdfxml , ldapi:ldapi683-skosrdf , ldapi:ldapi683-json ;
        geolexica:hasLangCode  lang:jpn , lang:kor , lang:msa , lang:zho , lang:fra , lang:rus , lang:ara , lang:swe , lang:eng , lang:fin , lang:pol , lang:nld , lang:spa , lang:deu ;
