:terminological_dictionary
        a                      skos:Concept , owl:NamedIndividual ;
        rdfs:label             "terminological dictionary" ;
        dcterms:identifier     901 ;
        skos:altLabel          "технический словарь"@ru , "technical dictionary"@ko , "technical dictionary"@de , "technical dictionary"@ar , "technical dictionary"@en , "diccionario técnico"@es ;
        skos:definition        "коллекция терминологических записей, представляющих информацию, связанную с понятиями или обозначениями из одной или нескольких конкретных предметных областей"@ru , "collection of terminological entries presenting information related to concepts or designations from one or more specific subject fields"@en , "مجموعة من المدخلات المصطلحية تمثل معلومات متعلقة بمفاهيم او دلالات من مجال موضوع واحد محدد او أكثر"@ar , "하나 또는 그 이상의 특정 주제 분야의 지정한 것과 정의와 연관된 정보를 제시하는 용어들의 집합"@ko , "conjunto de entradas terminológicas que informan acerca de los conceptos o las designaciones de uno o varios campos temáticos"@es ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          901 ;
        skos:prefLabel         "diccionario terminológico"@es , "терминологический словарь"@ru , "terminological dictionary"@en , "معجم مصطلحي"@ar , "terminologisches Wörterbuch (Syn.: Fachwörterbuch)"@de , "용어 사전"@ko ;
        lang:deu-note          notes:DefnTransUnavail ;
        geolexica:hasInfo      info:Info901 ;
        geolexica:hasLDAPI     ldapi:ldapi901-jsonld , ldapi:ldapi901-skosrdf , ldapi:ldapi901-rdfxml , ldapi:ldapi901-json ;
        geolexica:hasLangCode  lang:fra , lang:kor , lang:deu , lang:jpn , lang:rus , lang:ara , lang:fin , lang:nld , lang:swe , lang:pol , lang:eng , lang:zho , lang:spa , lang:msa ;
        geolexica:hasOrigin    lang:finRegistry , lang:msaRegistry , lang:engRegistry , lang:araRegistry , lang:jpnRegistry , lang:fraRegistry , lang:korRegistry , lang:rusRegistry , lang:zhoRegistry , lang:deuRegistry , lang:spaRegistry , lang:sweRegistry , lang:nldRegistry , lang:polRegistry ;
