:abstract_root_programming
        a                      owl:NamedIndividual , skos:Concept ;
        rdfs:label             "abstract root (programming)" ;
        dcterms:identifier     2028 ;
        skos:definition        "common root classifier of a category which is a superclass of any other classifier in the category"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2028 ;
        skos:prefLabel         "abstract root (programming)"@en ;
        lang:eng-note          "Note to entry: The class Any in some programming languages is the abstract root of all classes. Thus, it is the de facto union of all classes. In this document, Geometry is the (named and explicit) abstract root for all geometry objects. In the package Geometry and any of its subpackages (including those in its Requirements Classes), any interface will be a subtype of Geometry either directly or transitively."@en ;
        geolexica:hasInfo      info:Info2028 ;
        geolexica:hasLDAPI     ldapi:ldapi2028-jsonld , ldapi:ldapi2028-skosrdf , ldapi:ldapi2028-rdfxml , ldapi:ldapi2028-json ;
        geolexica:hasLangCode  lang:spa , lang:rus , lang:kor , lang:ara , lang:pol , lang:zho , lang:swe , lang:msa , lang:deu , lang:fra , lang:jpn , lang:nld , lang:fin , lang:eng ;
        geolexica:hasOrigin    lang:fraRegistry , lang:polRegistry , lang:araRegistry , lang:rusRegistry , lang:korRegistry , lang:sweRegistry , lang:spaRegistry , lang:nldRegistry , lang:jpnRegistry , lang:zhoRegistry , lang:deuRegistry , lang:msaRegistry , lang:engRegistry , lang:finRegistry ;
        geolexica:hasReview    reviews:Review2028 .

