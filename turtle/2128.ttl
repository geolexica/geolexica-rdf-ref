:sub_element_postal_address
        a                      skos:Concept , owl:NamedIndividual ;
        rdfs:label             "sub-element (postal address)" ;
        dcterms:identifier     2128 ;
        skos:altLabel          "postal address sub-element (postal address)"@en ;
        skos:definition        "identifier of either a sub-division of a postal address element value or one of multiple occurrences of an element in a postal address"@en ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          2128 ;
        skos:prefLabel         "sub-element (postal address)"@en ;
        lang:eng-note          "Note 3 to entry: For convenience, the preferred term “postal address sub-element” has been shortened to the admitted term “sub-element” throughout this document."@en , "Note 1 to entry: Postal address sub-elements are used to facilitate postal address rendering, database storage and related technical needs and should not be considered as specific cases of postal address components."@en , "Note 2 to entry: Postal address sub-elements are further described in 6.5."@en ;
        geolexica:hasInfo      info:Info2128 ;
        geolexica:hasLDAPI     ldapi:ldapi2128-json , ldapi:ldapi2128-jsonld , ldapi:ldapi2128-rdfxml , ldapi:ldapi2128-skosrdf ;
        geolexica:hasLangCode  lang:fin , lang:rus , lang:eng , lang:pol , lang:msa , lang:deu , lang:spa , lang:jpn , lang:nld , lang:zho , lang:swe , lang:fra , lang:kor , lang:ara ;
