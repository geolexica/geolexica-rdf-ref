:frame_sensor  a               owl:NamedIndividual , skos:Concept ;
        rdfs:label             "frame sensor" ;
        dcterms:identifier     939 ;
        skos:definition        "sensor that detects and collects all of the data for an image (frame / rectangle) at an instant of time"@en , "Sensor, der alle Daten einer Bildmatrix gleichzeitig aufnimmt"@de , "Датчик, который обнаруживает и собирает все данные для изображения (кадра / прямоугольника) в момент времени"@ru , "순간적으로 영상(프레임/직사각형)에 대한 자료 전부를 탐지 및 수집하는 센서"@ko , "sensor som detekterar och samlar in en bildrutas alla data samtidigt"@sv , "sensor que detecta y captura todos los datos de una imagen (matriz/rectángulo) en un instante de tiempo"@es , "حساس لكشف وجمع كافة بيانات الصورة  (اطار /مستطيل) في لحظة من الزمن."@ar ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          939 ;
        skos:prefLabel         "프레임 센서"@ko , "датчик кадра"@ru , "bildrutesensor"@sv , "frame sensor"@en , "Rahmenkamera"@de , "حساس إطاري"@ar , "sensor matricial"@es ;
        geolexica:hasInfo      info:Info939 ;
        geolexica:hasLDAPI     ldapi:ldapi939-skosrdf , ldapi:ldapi939-json , ldapi:ldapi939-rdfxml , ldapi:ldapi939-jsonld ;
        geolexica:hasLangCode  lang:deu , lang:rus , lang:fra , lang:nld , lang:fin , lang:swe , lang:kor , lang:ara , lang:eng , lang:zho , lang:spa , lang:pol , lang:msa , lang:jpn ;
        geolexica:hasOrigin    lang:polRegistry , lang:engRegistry , lang:deuRegistry , lang:spaRegistry , lang:rusRegistry , lang:sweRegistry , lang:msaRegistry , lang:finRegistry , lang:jpnRegistry , lang:nldRegistry , lang:fraRegistry , lang:araRegistry , lang:zhoRegistry , lang:korRegistry ;
        geolexica:hasReview    reviews:Review939 .

