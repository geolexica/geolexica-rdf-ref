:service_metadata  a           skos:Concept , owl:NamedIndividual ;
        rdfs:label             "service metadata" ;
        dcterms:identifier     408 ;
        skos:definition        "métadonnées décrivant les opérations et les informations géographiques disponibles sur un serveur"@fr , "palvelimella saatavilla olevia toimintoja ja paikkatietoa kuvaava metatieto"@fi , "بيانات وصفية تصف العمليات والمعلومات الجغرافية المتوفرة على خادم معيَّن"@ar , "metadata describing the operations and geographic information available at a server"@en , "metadane opisujące operacje i informację geograficzną dostępną na serwerze"@pl , "Metadaten, welche die auf einem Server verfügbaren Operationen und geographischen Informationen beschreiben"@de , "metadatos que describen las operaciones y la información geográfica disponible en un servidor"@es , "서버에서 활용할 수 있는 연산과 지리 정보를 기술하는 메타데이터"@ko , "метаданные, описывающие операции и географическую информацию, которые доступны на сервере"@ru , "metadata som beskriver de operationer och den geodata som är tillgänglig via en server"@sv , "人机间、系统间的公共边界。"@zh ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          408 ;
        skos:prefLabel         "paikkatietopalveluja kuvaileva metatieto"@fi , "服务接口"@zh , "Servicemetadaten"@de , "서비스 메타데이터"@ko , "metadata för tjänster"@sv , "metadane usługi"@pl , "metadatos de servicio"@es , "метаданные сервиса"@ru , "métadonnées de service"@fr , "service metadata"@en , "بيانات وصفية للخدمة"@ar ;
        lang:fra-note          notes:NoteTransDiff ;
        geolexica:hasInfo      info:Info408 ;
        geolexica:hasLDAPI     ldapi:ldapi408-json , ldapi:ldapi408-rdfxml , ldapi:ldapi408-skosrdf , ldapi:ldapi408-jsonld ;
        geolexica:hasLangCode  lang:fra , lang:jpn , lang:ara , lang:kor , lang:zho , lang:deu , lang:spa , lang:pol , lang:rus , lang:eng , lang:msa , lang:swe , lang:fin , lang:nld ;
