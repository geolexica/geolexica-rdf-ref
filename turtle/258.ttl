:isolated_node  a              skos:Concept , owl:NamedIndividual ;
        rdfs:label             "isolated node" ;
        dcterms:identifier     258 ;
        skos:definition        "어느 변에도 결합되어 있지 않은 절점"@ko , "węzeł nie związany z żadną krawędzią"@pl , "不与任何边相连的结点。"@zh , "عقدة ليست مرتبطة بحافه"@ar , "node not related to any edge"@en , "узел, не связанный ни с одним из ребер"@ru , "nœud n&#39;étant relié à aucune arête"@fr , "nod som inte är relaterad till någon länk"@sv , "nod yang tidak berkaitan dengan sebarang hujung"@ms , "solmupiste, joka ei liity mihinkään linkkiin"@fi , "nodo que no está relacionado con nigún arco"@es , "どのエッジとも関係しないノード。"@ja ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          258 ;
        skos:prefLabel         "孤立ノード"@ja , "węzeł izolowany"@pl , "nodo aislado"@es , "고립 절점"@ko , "изолированный узел"@ru , "isolated node"@en , "Einzelknoten"@de , "isolerad nod"@sv , "孤立结点"@zh , "عقدة معزولة"@ar , "nœud isolé"@fr , "nod terasing"@ms , "erillinen solmu"@fi ;
        lang:lang-deu          notes:DefnTransUnavail ;
        geolexica:hasInfo      info:Info258 ;
        geolexica:hasLDAPI     ldapi:ldapi258-json , ldapi:ldapi258-rdfxml , ldapi:ldapi258-skosrdf , ldapi:ldapi258-jsonld ;
        geolexica:hasLangCode  lang:pol , lang:rus , lang:deu , lang:swe , lang:msa , lang:zho , lang:fra , lang:ara , lang:kor , lang:eng , lang:spa , lang:fin ;
