:simple_register  a            owl:NamedIndividual , skos:Concept ;
        rdfs:label             "simple register" ;
        dcterms:identifier     1670 ;
        skos:definition        "register containing items of a single item class"@en , "registro que contiene ítems de una sola clase"@es ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          1670 ;
        skos:prefLabel         "simple register"@en , "registro simple"@es ;
        geolexica:examples     "EJEMPLO Un registro que contiene entradas terminológicas en un único idioma específicado."@es , "EXAMPLE A register containing terminological entries in a single specified language."@en ;
        geolexica:hasInfo      info:Info1670 ;
        geolexica:hasLDAPI     ldapi:ldapi1670-skosrdf , ldapi:ldapi1670-jsonld , ldapi:ldapi1670-rdfxml , ldapi:ldapi1670-json ;
        geolexica:hasLangCode  lang:zho , lang:ara , lang:fra , lang:swe , lang:spa , lang:kor , lang:eng , lang:jpn , lang:msa , lang:deu , lang:fin , lang:rus , lang:pol , lang:nld ;
