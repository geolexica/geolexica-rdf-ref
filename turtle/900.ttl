:terminological_data  a        skos:Concept , owl:NamedIndividual ;
        rdfs:label             "terminological data" ;
        dcterms:identifier     900 ;
        skos:definition        "información relativa a los conceptos o a sus designaciones"@es , "data related to concepts or their designations"@en , "данные, относящиеся к понятиямили их обозначениям"@ru , "بيانات متعلقة بمفاهيم أو دلالاتها"@ar , "개념 또는 지정된 바와 관계된 자료"@ko ;
        skos:inScheme          geolexica:GeolexicaConceptScheme ;
        skos:notation          900 ;
        skos:prefLabel         "terminological data"@en , "terminologische Daten"@de , "용어 자료"@ko , "información terminológica"@es , "بيانات مصطلحية"@ar , "терминологические данные"@ru ;
        lang:ara-note          "ملاحظة: البيانات المصطلحية الأكثر شيوعا هي المصطلح كمادة مدرجة في المعجم ، وتعريفه ، والملاحظة ، والإعراب النحوي ، وتوضيح المادة ، وتعريف اللغة ، وتعريف الدولة أو البلد ، وتعريف المصدر."@ar ;
        lang:deu-note          notes:DefnTransUnavail , notes:NoteTransUnavail ;
        lang:eng-note          "Note to entry: The more common terminological data include entry term, definition, note, grammatical label, subject label, language identifier, country identifier and source identifier. "@en ;
        lang:kor-note          notes:NoteTransUnavail ;
        lang:rus-note          "Примечание: В общем случае терминологические данные включают: термин, определение, примечание, грамматическую пометку, метку предмета, идентификатор языка, идентификатор страны и идентификатор источника"@ru ;
        lang:spa-note          "Nota: La información terminológica suele incluir la entrada, la definición, la nota, la marca gramatical, la marca temática, el identificador de lengua, el identificador de país y el identificador de fuente."@es ;
        geolexica:hasInfo      info:Info900 ;
        geolexica:hasLDAPI     ldapi:ldapi900-json , ldapi:ldapi900-skosrdf , ldapi:ldapi900-rdfxml , ldapi:ldapi900-jsonld ;
        geolexica:hasLangCode  lang:jpn , lang:spa , lang:fra , lang:pol , lang:ara , lang:nld , lang:zho , lang:rus , lang:swe , lang:kor , lang:msa , lang:eng , lang:deu , lang:fin ;
        geolexica:hasOrigin    lang:korRegistry , lang:sweRegistry , lang:araRegistry , lang:finRegistry , lang:engRegistry , lang:jpnRegistry , lang:msaRegistry , lang:nldRegistry , lang:zhoRegistry , lang:polRegistry , lang:spaRegistry , lang:rusRegistry , lang:fraRegistry , lang:deuRegistry ;
        geolexica:hasReview    reviews:Review900 .

