        geolexica:hasLDAPI     ldapi:ldapi224-skosrdf , ldapi:ldapi224-jsonld , ldapi:ldapi224-rdfxml , ldapi:ldapi224-json ;
        geolexica:hasLangCode  lang:swe , lang:kor , lang:ara , lang:eng , lang:deu , lang:spa , lang:rus ;
        geolexica:hasOrigin    lang:spaRegistry , lang:sweRegistry , lang:engRegistry , lang:deuRegistry , lang:rusRegistry , lang:araRegistry , lang:korRegistry ;
        geolexica:hasReview    reviews:Review224 .

