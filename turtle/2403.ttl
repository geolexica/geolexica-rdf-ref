        geolexica:hasLDAPI     ldapi:ldapi2403-jsonld , ldapi:ldapi2403-json , ldapi:ldapi2403-skosrdf , ldapi:ldapi2403-rdfxml ;
        geolexica:hasLangCode  lang:jpn , lang:eng , lang:nld , lang:swe , lang:pol , lang:kor , lang:ara , lang:rus , lang:msa , lang:fra , lang:spa , lang:deu , lang:zho , lang:fin ;
